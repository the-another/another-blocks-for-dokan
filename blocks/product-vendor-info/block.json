{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "the-another/blocks-for-dokan-product-vendor-info",
	"title": "Product Vendor Info",
	"category": "dokan",
	"icon": "admin-users",
	"description": "Display vendor information on product pages with customizable field blocks.",
	"keywords": [ "dokan", "vendor", "product", "info", "container" ],
	"textdomain": "dokan-blocks",
	"providesContext": {
		"dokan/vendor": "vendorData"
	},
	"supports": {
		"html": false,
		"align": true,
		"spacing": {
			"margin": true,
			"padding": true
		},
		"layout": {
			"allowSwitching": true,
			"allowInheriting": false,
			"default": {
				"type": "flex",
				"orientation": "horizontal"
			}
		}
	},
	"attributes": {
		"productId": {
			"type": "number",
			"default": 0
		},
		"vendorData": {
			"type": "object",
			"default": null
		}
	},
	"example": {
		"attributes": {
			"productId": 0
		}
	}
}
