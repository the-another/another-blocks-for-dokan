{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "the-another/blocks-for-dokan-vendor-store-terms-conditions",
	"title": "Vendor Store Terms & Conditions",
	"category": "dokan",
	"icon": "media-document",
	"description": "Display vendor's terms and conditions content.",
	"keywords": [ "dokan", "vendor", "store", "terms", "conditions", "toc", "policy" ],
	"textdomain": "dokan-blocks",
	"supports": {
		"html": false,
		"align": true,
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true
		},
		"color": {
			"background": true,
			"text": true
		}
	},
	"attributes": {
		"vendorId": {
			"type": "number",
			"default": 0
		},
		"showTitle": {
			"type": "boolean",
			"default": true
		},
		"titleTag": {
			"type": "string",
			"default": "h2"
		}
	},
	"example": {
		"attributes": {
			"vendorId": 0,
			"showTitle": true
		}
	},
	"render": "theabd_render_vendor_store_terms_conditions_block"
}
