{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "the-another/dokan-store-sidebar",
	"title": "Store Sidebar",
	"category": "dokan",
	"icon": "sidebar",
	"description": "Display store sidebar with widgets.",
	"keywords": [ "dokan", "vendor", "store", "sidebar" ],
	"textdomain": "dokan-blocks",
	"supports": {
		"html": false,
		"align": true,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"vendorId": {
			"type": "number",
			"default": 0
		},
		"enableThemeSidebar": {
			"type": "boolean",
			"default": false
		}
	},
	"example": {
		"attributes": {
			"vendorId": 0
		}
	},
	"render": "dokan_render_store_sidebar_block"
}
