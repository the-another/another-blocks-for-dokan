{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "the-another/dokan-store-contact-form",
	"title": "Store Contact Form",
	"category": "dokan",
	"icon": "email",
	"description": "Contact vendor form using Dokan's contact seller functionality.",
	"keywords": [ "dokan", "vendor", "store", "contact", "form", "email" ],
	"textdomain": "dokan-blocks",
	"supports": {
		"html": false,
		"align": true,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"usesContext": [ "dokan/vendorId", "dokan/vendor" ],
	"attributes": {
		"vendorId": {
			"type": "number",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"vendorId": 0
		}
	},
	"render": "dokan_render_store_contact_form_block"
}
